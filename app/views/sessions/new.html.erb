<h3>If you registered, please login</h3>
<h3>If not, please sign up for free</h3>
<%= form_tag sessions_path, method: 'post' do %>
<p>
<div class="div">
	<%= label_tag :email %>
	<%= text_field_tag :email %>
</div>
<div class="div">
	<%= label_tag :password %>
	<%= password_field_tag :password %>
</div>
<%= submit_tag "Login" %>
</p>
<% end %>
<!DOCTYPE html>
<html>
<head>
  <title>CsTemplate</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<br><br><%= flash_messages %> </br></br>
  <nav class="navbar navbar-fixed-top navbar-dark bg-inverse">
    <a class="navbar-brand" href="<%= root_path %>">Coder Ticket</a>
    <ul class="nav navbar-nav">
      <% unless signed_in? %>
        <li class="nav-item">
          <%= link_to 'Sign In', login_path, class: 'nav-link'%>
        </li>
        <li class="nav-item">
          <%= link_to 'Sign Up', new_user_path, class: 'nav-link'%>
        </li>
      <% else %>
        <li class="nav-item">
          <%= link_to 'My Events', created_events_path, class: 'nav-link'%>
          </li>
        <li class="nav-item">
          <%= link_to 'Create Event', new_event_path, class: 'nav-link'%>
        </li>
        <li class="nav-item">
          <%= link_to 'Create Venue', new_venue_path, class: 'nav-link'%>
        </li>
        <li class="nav-item">
          <%= link_to 'Sign out', sign_out_path, class: 'nav-link'%>
        </li>
      <% end %>

    </ul>
  </nav>  

<%= yield %>

</body>
</html>
